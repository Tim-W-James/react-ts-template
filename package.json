{
  "name": "repo_name",
  "version": "0.0.0",
  "description": "project_description :rocket:",
  "author": "Tim James",
  "license": "SEE LICENSE IN LICENSE.txt",
  "homepage": "https://github.com/Tim-W-James/repo_name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tim-W-James/repo_name.git"
  },
  "keywords": [
    "project_keywords"
  ],
  "bugs": {
    "url": "https://github.com/Tim-W-James/repo_name/issues"
  },
  "type": "module",
  "main": "src/main.ts",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "dev:open": "vite --open",
    "dev:https": "vite --https",
    "start": "npm run dev",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "lint": "eslint src && stylelint './src/**/*.{css,scss}'",
    "format": "eslint src --fix && eslint tests --fix && stylelint './src/**/*.{css,scss}' --fix",
    "format:all": "npm run format && prettier --write './**/*.{json,yaml,yml,ts,js,cjs,md}'",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "./src/**/*.tsx": [
      "eslint --fix",
      "eslint"
    ],
    "./tests/**/*.tsx": [
      "eslint --fix",
      "eslint"
    ],
    "./src/**/*.{css,scss}": [
      "stylelint --fix",
      "stylelint"
    ],
    "./**/*.md": "prettier --write",
    "./**/*.{json,yaml,yml,ts,js,cjs}": "prettier --write"
  },
  "devDependencies": {
    "@types/node": "^17.0.38",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitest/ui": "^0.13.0",
    "c8": "^7.11.3",
    "debug": "^4.3.4",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-vitest": "^0.0.8",
    "happy-dom": "^5.0.0",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "react-test-renderer": "^18.1.0",
    "sass": "^1.52.3",
    "stylelint": "^14.9.1",
    "stylelint-config-idiomatic-order": "^8.1.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "unplugin-auto-import": "^0.8.6",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.6.1",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.13.0"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "react": "^18.1.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "readonly-types": "^3.0.0",
    "source-map-support": "^0.5.21",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
